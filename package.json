{
  "name": "latest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint \"src/**/*.{js,ts,vue}\"",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,md,json}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,vue}\" --fix",
    "prepare": "husky",
    "type": "vue-tsc --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "core-js": "^3.8.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jwt-decode": "^4.0.0",
    "lucide-vue-next": "^0.507.0",
    "pinia": "^3.0.2",
    "vue": "^3.2.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@types/jest": "^27.0.1",
    "@types/jwt-decode": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "cypress": "^14.3.2",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^9.1.7",
    "jest": "^27.0.5",
    "lint-staged": "^15.5.1",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  }
}
